<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="11dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/ic_background_register"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Ingrese los nuevos datos "
            android:layout_marginStart="16dp"
            android:textColor="@color/gray_text"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationY="20dp" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Nombre Completo"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="50dp" />

        <EditText
            android:id="@+id/editTextTextPersonName7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/rounded"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:minHeight="40dp"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:translationY="55dp" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Correo electrónico"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="70dp" />

        <EditText
            android:id="@+id/editTextTextEmailAddress5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/rounded"
            android:ems="10"
            android:gravity="center"
            android:inputType="textEmailAddress"
            android:minHeight="40dp"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:translationY="75dp" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Teléfono móvil"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="90dp" />

        <EditText
            android:id="@+id/editTextPhone5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/rounded"
            android:ems="10"
            android:gravity="center"
            android:inputType="phone"
            android:minHeight="40dp"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:translationY="95dp" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Dirección de domicilio"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:textStyle="bold"
            android:translationY="110dp" />

        <EditText
            android:id="@+id/editTextTextPersonName8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/rounded"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:minHeight="40dp"
            android:textColor="@color/gray_text"
            android:textSize="16sp"
            android:translationY="115dp" />

        <Button
            android:id="@+id/button11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:text="Actualizar"
            android:translationY="150dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
